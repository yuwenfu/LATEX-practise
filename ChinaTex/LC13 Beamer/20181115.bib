@thesis{.2004b,
 author = {陈忠民},
 year = {2004},
 title = {高Q值RF-MEMS平面电感的研究},
 keywords = {电镀Cu;电感;品质因数;射频;微电子机械系统;氧化多孔硅},
 abstract = {射频微电子机械系统(RF-MEMS)技术在射频(RF)通信领域的应用正在得到广泛的重视。作为射频电路的重要元件，高Q值电感已经成为RF-MEMS器件研究中的热点。本论文对在不同硅衬底上获得高Q值电感进行了深入的研究；提出了一种在低阻硅衬底上形成氧化多孔硅厚膜来隔离硅衬底损耗的硅基平面电感结构；最后分别在氧化多孔硅厚膜隔离的低阻硅衬底和高阻硅衬底上制作出了高Q值Cu电感。},
 type = {硕士},
 institution = {清华大学},
 shorthand = {陈忠民 2004}
}


@article{.2004d,
 author = {鲍雅萍},
 year = {2004},
 title = {微机电系统性能检测的分析与研究},
 keywords = {传感器;检测;微机电系统},
 pages = {14--16},
 pagination = {page},
 issn = {1671-928X},
 journaltitle = {安阳大学学报},
 shorthand = {鲍雅萍 2004},
 number = {02},
 abstract = {微机电系统是一种集微电子电路和微机械致动器的微小器件,微传感器的性能检测可通过对系统输入量和输出量的检测,选择合适的测试仪器和测试方法来实现。根据角频率的推导,采用频率响应方法,其结果比通过传递函数数值分析得到的谐振角频率精确得多。}
}


@misc{.2005b,
 year = {2005},
 title = {MEMS多层压印工艺中的视频图像对准系统},
 keywords = {对准系统;多层压印;视频图像;微电子机械系统},
 abstract = {针对基于微压印成形三维MEMS器件制造工艺多层压印的需要,开发了一套基于视频图像对正原理的压印光刻对准系统。硬件系统以显微镜头、CCD、图像采集卡和精密工作台为核心组成。为了使系统能适应不同厚度器件的对准,设计了长工作距离的显微镜头并使用透明模板。软件中采用亚像素相关模板匹配算法进行定位运算,算法误差小于0.2\textgreek{m}m。通过采用选择有效区域和粗-细搜索法的策略对算法进行优化,提高了运算效率。系统性能实验分析表明,系统分辨精度达到0.2\textgreek{m}m,能够满足2\textgreek{m}m的对准精度要求。},
 shorthand = {2005},
 number = {10},
 pagerange = {84-88},
 isbn = {1003-501X}
}


